    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@tsconfig/create-react-app": "2.0.1",
    "@types/node": "16.18.40",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "browser-sync": "^2.29.3",
    "cross-env": "7.0.3",
    "esbuild": "0.14.54",
    "esbuild-plugin-copy": "1.3.0",
    "esbuild-sass-plugin": "2.3.2",
    "get-port": "^7.0.0",
