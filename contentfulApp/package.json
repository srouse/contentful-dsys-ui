{
  "name": "my-first-app",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.tsx",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "start": "node ./scripts/server.mjs",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@contentful/app-sdk": "^4.22.1",
    "@contentful/f36-components": "4.48.0",
    "@contentful/f36-tokens": "4.0.2",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.4",
    "chalk": "^5.3.0",
    "contentful": "^9.2.9",
    "contentful-auto-ui": "file:../build/_webcomps",
    "contentful-management": "10.39.2",
    "emotion": "10.0.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "latest",
